cat > .gitignore << 'EOF'
# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
venv/
env/
ENV/

# IDEs
.vscode/
.idea/
*.swp
*.swo

# Data
data/uploads/*
data/processed/*
!data/uploads/.gitkeep
!data/processed/.gitkeep

# Logs
logs/*.log
*.log

# Environment
.env
.env.local

# OS
.DS_Store
Thumbs.db
EOF